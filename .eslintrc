{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true,
    "node": true
  },
  "ecmaFeatures": {
    "classes": true,
    "jsx": true
  },
  "globals": {
    "expect": true
  },
  "rules": {
    "comma-dangle": 1,
    "no-unused-vars": 1
  },
  "plugins": [
    "react"
  ]
}
